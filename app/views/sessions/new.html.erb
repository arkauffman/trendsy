<h3> Log In </h3>
<p id="alert"><%= alert %></p>
<%= form_tag sessions_path do %>
    <div class="row">
        <div class="input-field col s6 offset-s1">
            <%= label_tag :email %>
            <%= text_field_tag :email, params[:email] %>
        </div>
    </div>
    <div class="row">
        <div class="input-field col s6 offset-s1">
            <%= label_tag :password %>
            <%= password_field_tag :password %>
        </div>
    </div>
    <div class="actions"><%= submit_tag "Log In" %></div>
<% end %>

